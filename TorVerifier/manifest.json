{
  "manifest_version": 3,
  "name": "Privacy Ledger",
  "version": "1.0",
  "description": "An app that utilizes a Tor proxy wrapper to anonymize traffic and display all active webpage trackers to the user.",
  "permissions": [
    "nativeMessaging",
    "storage",
    "proxy",
    "webRequest",
    "tabs"
  ],
  "host_permissions":[
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/onion.png",
    "48": "icons/onion.png",
    "128": "icons/onion.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/onion.png",
      "32": "icons/onion.png",
      "48": "icons/onion.png",
      "128": "icons/onion.png"
    }
  }
}
